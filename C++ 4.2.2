#include <iostream>
#include <math.h>
using namespace  std;

int prime(int n);
int main() {
		for (int i = 2; i <= 20; ++i) {
			if (prime(i))
				cout << i << " is prime\n";
			else
				cout << i << " is not prime\n";
		}
	return 0;
}
int prime(int n) {
	double sqrt_of_n = sqrt(n);
	for (int i = 2; i <= sqrt_of_n; ++i) {
		if (n % i == 0)
			return false;
		else
			return true;
	}
}
