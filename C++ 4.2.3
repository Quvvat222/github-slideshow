#include <iostream>
#include <math.h>
using namespace  std;

int prime(int n);
int main() {
	int n = 1000000000;
	    for(int i = 1; i<=n; n++) {
			if (prime(n)) {
				cout << n << " is prime\n";
				break;
			}
			else
				cout << n << " is not prime\n";
		}
	return 0;
}
int prime(int n) {
	double sqrt_of_n = sqrt(n);
	for (int i = 2; i <= sqrt_of_n; ++i) {
		if (n % i == 0)
			return false;
		else
			return true;
	}
}
